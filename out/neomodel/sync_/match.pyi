@dataclass
class RelationNameResolver:
    relation: str
    def resolve(self, qbuilder: QueryBuilder) -> str: ...

@dataclass
class NodeNameResolver:
    node: str
    def resolve(self, qbuilder: QueryBuilder) -> str: ...

@dataclass
class BaseFunction:
    input_name: str | BaseFunction | NodeNameResolver | RelationNameResolver
    def __post_init__(self) -> None: ...
